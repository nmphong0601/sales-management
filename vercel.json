{
  "version": 2,
   "builds": [
       {
           "src": "server.js",
           "use": "@now/node"
       }
   ],
   "routes": [
       {
           "src": "/(.*)",
           "dest": "server.js"
       },
       {
           "src": "/api/v1/(.*)",
           "dest": "/routes/api/v1/index.js"
       }
   ]
}
