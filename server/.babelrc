{
  "presets": ["@babel/preset-env"],
  "env": {
    "production": {
      "plugins": [
        [
          "inline-dotenv",
          {
            "path": ".env.production"
          }
        ]
      ]
    },
    "development": {
      "plugins": [
        [
          "inline-dotenv",
          {
            "path": ".env.development"
          }
        ]
      ]
    }
  },
  "plugins": [
    "@babel/plugin-transform-runtime",
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-proposal-object-rest-spread"
  ]
}
